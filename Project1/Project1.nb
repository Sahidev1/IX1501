Cell["\<\
Task 5: Determine the number of trials needed to keep the relative error \
below 10%. Provide reasoning.\
\>", "Subsection",
 CellChangeTimes->{{3.7769686751205697`*^9, 3.776968720943965*^9}, {
   3.776968950644105*^9, 3.7769689792296224`*^9}, {3.777224905807621*^9, 
   3.77722491149018*^9}, 3.902832279880934*^9, 3.902832525650503*^9, 
   3.902833225135029*^9},ExpressionUUID->"b8d00377-161e-4337-b6b6-\
6c9030ea147d"],

Cell["\<\
To determine the number of trials needed to keep the relative error below \
10%, we can iterate through different trial numbers and stop when the \
relative error is less than 10%. \
\>", "Text",
 CellChangeTimes->{{3.777221488439082*^9, 3.7772215098326426`*^9}, 
   3.77722589721934*^9, 3.7772259585184555`*^9, {3.902832381209097*^9, 
   3.9028324113002157`*^9}, {3.9028325630793867`*^9, 3.9028325789477777`*^9}, 
   3.902834500559328*^9, {3.90283825159239*^9, 
   3.90283826967328*^9}},ExpressionUUID->"9cd4bd1a-b107-4ccc-978b-\
cc4acb4df335"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"desiredRelativeError", "=", "0.1"}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"Desired", " ", "relative", " ", "error", " ", "threshold"}], 
   "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"numTrials", "=", "10"}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"Initial", " ", "number", " ", "of", " ", "trials"}], "*)"}], "\n",
   "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Initialize", " ", "a", " ", "variable", " ", "to", " ", "store", " ", 
    "the", " ", "relative", " ", "error"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"relativeError", "=", "1.0"}], ";"}], "\n", "\[IndentingNewLine]", 
  
  RowBox[{"(*", 
   RowBox[{
   "Continue", " ", "until", " ", "the", " ", "relative", " ", "error", " ", 
    "is", " ", "below", " ", "the", " ", "threshold"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"While", "[", 
  RowBox[{
   RowBox[{"relativeError", ">", "desiredRelativeError"}], ",", 
   RowBox[{"(*", 
    RowBox[{"Simulate", " ", "numTrials", " ", "games"}], "*)"}], 
   RowBox[{
    RowBox[{"results", "=", 
     RowBox[{"Table", "[", 
      RowBox[{"(*", 
       RowBox[{"Simulate", " ", "a", " ", "single", " ", "game"}], "*)"}], 
      RowBox[{
       RowBox[{
        RowBox[{"diceResults", "=", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"1", ",", "n"}], "}"}], ",", "5"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"S", "=", 
         RowBox[{"Total", "[", "diceResults", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"S", "<=", "10"}], "||", 
         RowBox[{"S", ">=", "45"}]}]}], ",", 
       RowBox[{"(*", 
        RowBox[{"Check", " ", "if", " ", 
         RowBox[{"it", "'"}], "s", " ", "a", " ", "win"}], "*)"}], 
       RowBox[{"{", "numTrials", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Calculate", " ", "the", " ", "winning", " ", "probability", " ", 
      "using", " ", "simulation"}], "*)"}], 
    RowBox[{"winningProbabilityMC", "=", 
     RowBox[{
      RowBox[{"Total", "[", "results", "]"}], "/", "numTrials"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Calculate", " ", "the", " ", "relative", " ", "error"}], "*)"}], 
    RowBox[{"relativeError", "=", 
     RowBox[{"Abs", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"winningProbabilityMC", "-", "winningProbability"}], ")"}], 
       "/", "winningProbability"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Double", " ", "the", " ", "number", " ", "of", " ", "trials", " ", 
      "for", " ", "the", " ", "next", " ", "iteration"}], "*)"}], 
    RowBox[{"numTrials", "*=", "2"}], ";"}]}], "\n", "]"}]}], "Input",
 CellChangeTimes->{{3.902832999721644*^9, 3.902833002580904*^9}, {
