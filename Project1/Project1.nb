   3.902834373400934*^9}},ExpressionUUID->"33174136-fb73-4784-9ba8-\
a354f3792af1"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"trialCounts", "=", 
   RowBox[{"{", 
    RowBox[{"10", ",", "100", ",", "1000", ",", "10000", ",", "100000"}], 
    "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"estimatedProbabilities", "=", 
    RowBox[{"{", "}"}]}], ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"numTrials", "=", "10"}], ",", 
    RowBox[{"numTrials", "<=", "100000"}], ",", 
    RowBox[{"numTrials", "*=", "10"}], ",", 
    RowBox[{
     RowBox[{"winCount", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<=", "numTrials"}], ",", 
        RowBox[{"i", "++"}], ",", 
        RowBox[{
         RowBox[{"diceRolls", "=", 
          RowBox[{
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "4"}], "}"}], ",", "1"}], "]"}], "~", "Join", 
           "~", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "6"}], "}"}], ",", "1"}], "]"}], "~", "Join", 
           "~", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "8"}], "}"}], ",", "1"}], "]"}], "~", "Join", 
           "~", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "12"}], "}"}], ",", "1"}], "]"}], "~", "Join",
            "~", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "20"}], "}"}], ",", "1"}], "]"}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"totalSum", "=", 
          RowBox[{"Total", "[", "diceRolls", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"totalSum", "<=", "10"}], "||", 
            RowBox[{"totalSum", ">=", "45"}]}], ",", 
           RowBox[{"winCount", "++"}]}], "]"}], ";"}]}], "]"}], " ", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"estimatedProbabilities", ",", 
        RowBox[{"winCount", "/", "numTrials"}]}], "]"}]}], ";"}]}], "]"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{"ListLogPlot", "[", 
  RowBox[{
   RowBox[{"Transpose", "[", 
