Cell["\<\
Task 3: Obtain the probability of winning the game with 1000 trials (Monte \
Carlo simulation)\
\>", "Subsection",
 CellChangeTimes->{{3.7769686751205697`*^9, 3.776968720943965*^9}, {
   3.7769689193598886`*^9, 3.7769689370694237`*^9}, 
   3.9028321254950857`*^9},ExpressionUUID->"1fe66896-c816-474b-a84b-\
ee18f42a2062"],

Cell["\<\
For this task, we\[CloseCurlyQuote]ll use Monte Carlo simulation to estimate \
the probability of winning the game with 1000 trials. We\[CloseCurlyQuote]ll \
simulate the rolling of five dice and count how many times we win.\
\>", "Text",
 CellChangeTimes->{{3.7772211083183427`*^9, 3.7772211130540495`*^9}, {
   3.7772212161572037`*^9, 3.777221216513251*^9}, {3.7772233323715982`*^9, 
   3.7772233759150677`*^9}, {3.7772234149602013`*^9, 
   3.7772235172677965`*^9}, {3.777223556862294*^9, 3.777223707491474*^9}, 
   3.902832183812895*^9, 
   3.902834335051257*^9},ExpressionUUID->"642949a1-790f-4940-8841-\
08c9b5a355c7"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"numTrials", "=", "1000"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"winCount", "=", "0"}], ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "<=", "numTrials"}], ",", 
    RowBox[{"i", "++"}], ",", 
    RowBox[{
     RowBox[{"diceRolls", "=", 
      RowBox[{
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "4"}], "}"}], ",", "1"}], "]"}], "~", "Join", "~", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "6"}], "}"}], ",", "1"}], "]"}], "~", "Join", "~", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "8"}], "}"}], ",", "1"}], "]"}], "~", "Join", "~", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "12"}], "}"}], ",", "1"}], "]"}], "~", "Join", 
       "~", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "20"}], "}"}], ",", "1"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"totalSum", "=", 
      RowBox[{"Total", "[", "diceRolls", "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"totalSum", "<=", "10"}], "||", 
        RowBox[{"totalSum", ">=", "45"}]}], ",", 
       RowBox[{"winCount", "++"}]}], "]"}], ";"}]}], "]"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{"estimatedProbability", "=", 
  RowBox[{"winCount", "/", "numTrials"}]}]}], "Input",
 CellChangeTimes->{{3.902832211279202*^9, 3.902832211286561*^9}, {
  3.902834303899682*^9, 3.9028343075649776`*^9}},
 Background->GrayLevel[0.85],
 CellLabel->
