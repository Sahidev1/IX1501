Cell[BoxData[
 RowBox[{
  RowBox[{"randSum", " ", ":=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"Total", "[", "#", "]"}], "&"}], "/@", 
    RowBox[{"{", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"RandomInteger", "[", 
        RowBox[{"{", 
         RowBox[{"1", ",", "#"}], "}"}], "]"}], "&"}], " ", "/@", " ", 
      RowBox[{"{", 
